extends html

block styles
  link(rel="stylesheet", href="../css/style.css?_=" + builddate)
  title Moment.js Unit Test Suite

block scripts
  script(src="../js/moment.js?_=" + builddate)
  script
    var require = function(){
        return moment;
    }
    var exports = {};
  script(src="../js/langs.js?_=" + builddate)
  script(src="../js/tests.js?_=" + builddate)

block content
  #header-row.row
    .span8
      .hero-unit
        h1 Moment.js
        h2 Unit test suite
        p#nodeunit-userAgent
    .span4 
      #nodeunit-banner.alert.alert-info &nbsp;
  .row
    .span12
      ol#nodeunit-tests
